{
  "expo": {
    "name": "AegisIQ",
    "slug": "AegisIQ",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "frontend",
    "icon": "./assets/images/lightAppIcon.png",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.AegisIQ",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#008cbb"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.AegisIQ"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#008cbb",
          "dark": {
            "backgroundColor": "#000000",
            "image": "./assets/images/darkLogo.png"
          }
        }
      ],
      [
        "expo-alternate-app-icons",
        [
          {
            "name": "IconLight",
            "ios": "./assets/images/lightAppIcon.png",
            "android": {
              "foregroundImage": "./assets/images/lightIcon(new).png",
              "backgroundColor": "#008cbb"
            }
          },
          {
            "name": "IconDark",
            "ios": "./assets/images/darkAppIcon.png",
            "android": {
              "foregroundImage": "./assets/images/darkIcon(new).png",
              "backgroundColor": "#000000"
            }
          }
        ]
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c9a32da5-6e6d-4674-96ae-549097d1958f"
      }
    }
  }
}
